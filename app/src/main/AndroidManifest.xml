<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.macshark.testapp" >

    <!-- To access Google+ APIs: -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.macshark.testapp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.macshark.testapp.ExerciseActivity"
            android:label="@string/title_activity_exercise" >
        </activity>
        <activity
            android:name="com.example.macshark.testapp.Exerciselist"
            android:label="@string/title_activity_exerciselist" >
        </activity>
        <activity
            android:name="com.example.macshark.testapp.Exercise2"
            android:label="@string/title_activity_exercise2" >
        </activity>
    </application>

</manifest>
